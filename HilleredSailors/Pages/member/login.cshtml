@page
@model HilleredSailors.Pages.member.loginModel
@{
}
<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <div asp-validation-summary="All" class="text-danger"></div>
        <label asp-for="@Model.Member.Name" class="control-label"></label>
        <input asp-for="@Model.Member.Name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="@Model.Member.Phone" class="control-label"></label>
        <input asp-for="@Model.Member.Phone" class="form-control" />
    </div
    <div class="formgroup">
        <input type="submit" value="Create Member" class="btn btn-primary" />
    </div>
</form>